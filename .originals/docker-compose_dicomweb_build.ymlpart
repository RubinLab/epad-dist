  mydicomweb:
    build: {dicomweb_dockerfiledir}
    container_name: epad_dicomweb
    # expose:
    #   - "{dicomweb_port}"
    # ports:
    #   - "{dicomweb_port}:8090"
    volumes:
      - ./production_dicomweb.js:/home/node/app/config/development.js
    depends_on:
      - mykeycloak
    healthcheck:
      test: ["CMD", "pm2", "show", "DICOMwebServer"]
      interval: 1m30s
      timeout: 1m
      retries: 10
    networks:
      - epad-network
