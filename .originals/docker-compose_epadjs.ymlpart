  myepadjs:
    image: rubinlab/epadjs:{epadjs_image}
    container_name: epad_js
    expose:
      - "{epadjs_port}"
    ports:
      - "{epadjs_port}:80"
    environment:
      PUBLIC_URL: "http://{host}"
      REACT_APP_BASE_URL: "{epadjs_baseurl}"
    volumes:
      - ./production_epadjs.json:/app/pu/config.json
      - ./production_keycloak.json:/app/public/keycloak.json
    depends_on:
      - myepadlite
    networks:
      - epad-network
